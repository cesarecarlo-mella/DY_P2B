  Get[FileNameJoin[{BasisDirectory[IF2], "UniqueSectors[IF2]"}]]
IF2 /: ZerojRule[IF2] := LiteRed`Private`jjj:j[IF2, __] :> 
   0 /; Module[{LiteRed`Private`jjs = 
       Rest[List @@ jSector[LiteRed`Private`jjj]]}, 
