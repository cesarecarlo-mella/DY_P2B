    (j[IF2, 0, -1, 0, 0, 0, 0, 0] - j[IF2, 0, 0, 0, 0, 0, 0, -1])/2, 
   sp[p4, p4] -> j[IF2, -1, 0, 0, 0, 0, 0, 0], 
   sp[p4, p5] -> -1/2*(m2*u - m2*z - u*z*j[IF2, -1, 0, 0, 0, 0, 0, 0] - 
