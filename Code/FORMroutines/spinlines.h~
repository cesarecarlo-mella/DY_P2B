****************************************************************************************
****************************************************************************************
****************************************************************************************
****************************************************************************************
****************************************************************************************
****************************************************************************************

***two external quark-qbar initial-initial final-final line
#if 'PROC' == VqqbQQb;
*** start with initial state spin-line. 
id pol(Qbar(97,p2,i97))=Search(97)*Spin(1,p2);
repeat;
*** quark of type q, use gv ga
id Search(n1?)*vQQg(mu1?,n1?,n2?)*Spin(1,?x2) =  Save(vQQg(mu1,n1,n2)) * Search(n2) * Spin(1,mu1,?x2);
id Search(n1?)*vQQV(n1?,nu1?,n2?)*Spin(1,?z2)  = Save(vQQV(n1,nu1,n2))*Search(n2) *(gv(ti)*Spin(1,nu1,?z2) + ga(ti)*eps(nu1,rho1,rho2,rho3)/6 *Spin(1,rho1,rho2,rho3,
?z2));
id Search(n1?)*pQuark(n1?,mom1?,n2?)*Spin(1,?x2)  =  Save(pQuark(n1,mom1,n2)) *Search(n2) * Spin(1,mom1,?x2) ;
endrepeat;
id Search(99)*pol(Quark(99, p1, i99))*Spin(1,?z2) = Spin(1,p1,?z2);
.sort
id Save(pQuark(?x))=pQuark(?x);
id Save(?x) = 1;
*****************
#message spinline 1 done
*****************
**** this spin line will close and sum over all flavor will be performed. Use NfVExt to remember to perform sum. Quark of type Q, use Gv and Ga
id pol(Qbar2(91,r2,i91))=Search(91)*Spin(2,r2)*Qbar2(91,r2);
repeat;
id Search(n1?)*vQQg(mu1?,n1?,n2?)*Spin(2,?x2) = Save(vQQg(mu1,n1,n2)) * Search(n2) * Spin(2,mu1,?x2);
id Search(n1?)*vQQV(n1?,nu1?,n2?)*Spin(2,?z2)  = Save(vQQV(n1,nu1,n2))*Search(n2) *(Gv(ti)*Spin(2,nu1,?z2) + Ga(ti)*eps(nu1,rho1,rho2,rho3)/6 *Spin(2,rho1,rho2,rho3,
?z2));
id Search(n1?)*pQuark(n1?,mom1?,n2?)*Spin(2,?x2)  =  Save(pQuark(n1,mom1,n2)) *Search(n2) * Spin(2,mom1,?x2) ;
endrepeat;
id Search(93)*pol(Quark2(93, r1, i93))*Spin(2,?z2) = Spin(2,r1,?z2)*Quark2(93, r1);
.sort
id Save(pQuark(?x))=pQuark(?x);
id Save(?x) = 1;
id Quark2(93, r1) = pol(Quark2(93,r1,i93));
id Qbar2(91, r2) = pol(Qbar2(91,r2,i91));
.sort
#endif


****************************************************************************************
****************************************************************************************
****************************************************************************************
****************************************************************************************
****************************************************************************************
****************************************************************************************

***two external quark-qbar initial-initial final-final line
#if 'PROC' == Vqqbqqb;
*** start with initial state qbar
id pol(Qbar(97,p2,i97))=Search(97)*Spin(1,p2);
repeat;
id Search(n1?)*vQQg(mu1?,n1?,n2?)*Spin(1,?x2) =  Save(vQQg(mu1,n1,n2)) * Search(n2) * Spin(1,mu1,?x2);
id Search(n1?)*vQQV(n1?,nu1?,n2?)*Spin(1,?z2)  = Save(vQQV(n1,nu1,n2))*Search(n2) *(gv(ti)*Spin(1,nu1,?z2) + ga(ti)*eps(nu1,rho1,rho2,rho3)/6 *Spin(1,rho1,rho2,rho3,
?z2));
id Search(n1?)*pQuark(n1?,mom1?,n2?)*Spin(1,?x2)  =  Save(pQuark(n1,mom1,n2)) *Search(n2) * Spin(1,mom1,?x2) ;
endrepeat;
*** either it finishes on an initial quark or it finishes on a final qbar: in my convention, it is always quark (incoming in vertices in form)
* in the first case I need to remenber to save the final state for the polarization sum when squaring
id Search(93)*pol(Quark(93, r1,i93))*Spin(1,?z2) = Spin(1,r1,?z2)*Quark(93, r1);
id Search(99)*pol(Quark(99,p1,i99))*Spin(1,?z2)  = Spin(1,p1,?z2) ; 
.sort
id Save(pQuark(?x))=pQuark(?x);
id Save(?x) = 1;
*****************
#message spinline 1 done
*****************
**** Second spin line
id pol(Qbar(91,r2,i91))=Search(91)*Spin(2,r2)*Qbar(91,r2);
repeat;
id Search(n1?)*vQQg(mu1?,n1?,n2?)*Spin(2,?x2) = Save(vQQg(mu1,n1,n2)) * Search(n2) * Spin(2,mu1,?x2);
id Search(n1?)*vQQV(n1?,nu1?,n2?)*Spin(2,?z2)  = Save(vQQV(n1,nu1,n2))*Search(n2) *(gv(ti)*Spin(2,nu1,?z2) + ga(ti)*eps(nu1,rho1,rho2,rho3)/6 *Spin(2,rho1,rho2,rho3,
?z2));
id Search(n1?)*pQuark(n1?,mom1?,n2?)*Spin(2,?x2)  =  Save(pQuark(n1,mom1,n2)) *Search(n2) * Spin(2,mom1,?x2) ;
endrepeat;
id Search(93)*pol(Quark(93, r1,i93))*Spin(2,?z2) = Spin(2,r1,?z2)*Quark(93, r1);
id Search(99)*pol(Quark(99, p1,i99))*Spin(2,?z2) = Spin(2,p1,?z2);
.sort
id Save(pQuark(?x))=pQuark(?x);
id Save(?x) = 1;
id Quark(93, r1) = pol(Quark(93,r1,i93));
id Qbar(91, r2) = pol(Qbar(91,r2,i91));
.sort
#endif


****************************************************************************************
****************************************************************************************
****************************************************************************************
****************************************************************************************
****************************************************************************************
****************************************************************************************
***two external quark-qbar initial-initial final-final line
#if 'PROC' == VqQqQ;
*** two final state qBar (they are incoming, so outgoing quark :) ). Lets start wit the first one.
id pol(Qbar(93,r1,i93))=Search(93)*Spin(1,r1)*Qbar(93,r1);
repeat;
id Search(n1?)*vQQg(mu1?,n1?,n2?)*Spin(1,?x2) =  Save(vQQg(mu1,n1,n2)) * Search(n2) * Spin(1,mu1,?x2);
id Search(n1?)*vQQV(n1?,nu1?,n2?)*Spin(1,?z2)  = Save(vQQV(n1,nu1,n2))*Search(n2) *(gv(ti)*Spin(1,nu1,?z2) + ga(ti)*eps(nu1,rho1,rho2,rho3)/6 *Spin(1,rho1,rho2,rho3,
?z2));
id Search(n1?)*pQuark(n1?,mom1?,n2?)*Spin(1,?x2)  =  Save(pQuark(n1,mom1,n2)) *Search(n2) * Spin(1,mom1,?x2) ;
endrepeat;
id Search(99)*pol(Quark(99, p1,i99))*Spin(1,?z2) = Spin(1,p1,?z2);
.sort
id Save(pQuark(?x))=pQuark(?x);
id Save(?x) = 1;
*****************
#message spinline 1 done
*****************
**** Second spin line
id pol(Qbar2(91,r2,i91))=Search(91)*Spin(2,r2)*Qbar2(91,r2);
repeat;
id Search(n1?)*vQQg(mu1?,n1?,n2?)*Spin(2,?x2) = Save(vQQg(mu1,n1,n2)) * Search(n2) * Spin(2,mu1,?x2);
id Search(n1?)*vQQV(n1?,nu1?,n2?)*Spin(2,?z2)  = Save(vQQV(n1,nu1,n2))*Search(n2) *(Gv(ti)*Spin(2,nu1,?z2) + Ga(ti)*eps(nu1,rho1,rho2,rho3)/6 *Spin(2,rho1,rho2,rho3,
?z2));
id Search(n1?)*pQuark(n1?,mom1?,n2?)*Spin(2,?x2)  =  Save(pQuark(n1,mom1,n2)) *Search(n2) * Spin(2,mom1,?x2) ;
endrepeat;
id Search(97)*pol(Quark2(97, p2,i97))*Spin(2,?z2) = Spin(2,p2,?z2);
.sort
id Save(pQuark(?x))=pQuark(?x);
id Save(?x) = 1;
id Qbar(93, r1) = pol(Qbar2(93,r1,i93));
id Qbar2(91, r2) = pol(Qbar2(91,r2,i91));
.sort
#endif



