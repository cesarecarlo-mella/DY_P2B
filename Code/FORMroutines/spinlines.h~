***two external quark-qbar initial-initial final-final line
#if 'PROC' == VqQbqQb;
*** two final state qBar (they are incoming, so outgoing quark :) ). Lets start wit the first one.
id pol(Qbar(93,r1,i93))=Search(93)*Spin(1,r1)*Qbar(93,r1);
repeat;
id Search(n1?)*vQQg(mu1?,n1?,n2?)*Spin(1,?x2) =  Save(vQQg(mu1,n1,n2)) * Search(n2) * Spin(1,mu1,?x2);
id Search(n1?)*vQQV(n1?,nu1?,n2?)*Spin(1,?z2)  = Save(vQQV(n1,nu1,n2))*Search(n2) *(gv(ti)*Spin(1,nu1,?z2) + ga(ti)*eps(nu1,rho1,rho2,rho3)/6 *Spin(1,rho1,rho2,rho3,
?z2));
id Search(n1?)*pQuark(n1?,mom1?,n2?)*Spin(1,?x2)  =  Save(pQuark(n1,mom1,n2)) *Search(n2) * Spin(1,mom1,?x2) ;
endrepeat;
id Search(99)*pol(Quark(99, p1,i99))*Spin(1,?z2) = Spin(1,p1,?z2);
.sort
id Save(pQuark(?x))=pQuark(?x);
id Save(?x) = 1;
*****************
#message spinline 1 done
*****************
**** Second spin line
id pol(Qbar2(97,p2,i97))=Search(97)*Spin(2,p2);
repeat;
id Search(n1?)*vQQg(mu1?,n1?,n2?)*Spin(2,?x2) = Save(vQQg(mu1,n1,n2)) * Search(n2) * Spin(2,mu1,?x2);
id Search(n1?)*vQQV(n1?,nu1?,n2?)*Spin(2,?z2)  = Save(vQQV(n1,nu1,n2))*Search(n2) *(Gv(ti)*Spin(2,nu1,?z2) + Ga(ti)*eps(nu1,rho1,rho2,rho3)/6 *Spin(2,rho1,rho2,rho3,
?z2));
id Search(n1?)*pQuark(n1?,mom1?,n2?)*Spin(2,?x2)  =  Save(pQuark(n1,mom1,n2)) *Search(n2) * Spin(2,mom1,?x2) ;
endrepeat;
id Search(91)*pol(Quark2(91, r2,i91))*Spin(2,?z2) = Spin(2,r2,?z2)*Quark2(91,r2);
.sort
id Save(pQuark(?x))=pQuark(?x);
id Save(?x) = 1;
id Qbar(93, r1) = pol(Qbar2(93,r1,i93));
id Quark2(91, r2) = pol(Quark2(91,r2,i91));
.sort
#endif



